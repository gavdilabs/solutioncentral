{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.gavdilabs.solutioncockpit",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.16.1",
      "toolsId": "510bf0f2-bef6-4d30-ba2d-d58bc55c7735"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/radar/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.131.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.gavdilabs.solutioncockpit.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "SoftwareSolutionList",
          "target": "SoftwareSolutionList"
        },
        {
          "pattern": "SoftwareSolution({key}):?query:",
          "name": "SoftwareSolutionObjectPage",
          "target": "SoftwareSolutionObjectPage"
        }
      ],
      "targets": {
        "SoftwareSolutionList": {
          "type": "Component",
          "id": "SoftwareSolutionList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/SoftwareSolution",
              "variantManagement": "None",
              "enhanceI18n": "i18n/customi18n.properties",
              "navigation": {
                "SoftwareSolution": {
                  "detail": {
                    "route": "SoftwareSolutionObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "columns": {
                    "DataField::description": {
                      "width": "300px"
                    },
                    "DataField::technologyType": {
                      "width": "100px"
                    },
                    "DataFieldForAnnotation::DataPoint::cleanCoreRatingIndicator": {
                      "width": "150px"
                    },
                    "DataFieldForAnnotation::DataPoint::codeQualityRatingIndicator": {
                      "width": "150px"
                    }
                  }
                }
              },
              "initialLoad": "Enabled"
            }
          }
        },
        "SoftwareSolutionObjectPage": {
          "type": "Component",
          "id": "SoftwareSolutionObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "enhanceI18n": "i18n/customi18n.properties",
              "editableHeaderContent": true,
              "contextPath": "/SoftwareSolution",
              "showRelatedApps": false,
              "controlConfiguration": {
                "Technologies/@com.sap.vocabularies.UI.v1.LineItem#Technologies": {
                  "tableSettings": {
                    "disableCopyToClipboard": true,
                    "enableExport": false,
                    "selectionMode": "Multi",
                    "type": "ResponsiveTable",
                    "enablePaste": false
                  },
                  "columns": {
                    "DataField::technology_ID": {
                      "width": "30%"
                    },
                    "DataField::technology::description": {
                      "width": "70%"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
