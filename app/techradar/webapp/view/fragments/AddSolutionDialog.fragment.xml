<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog	
		id="SoftwareSelect"	
		noDataText="No Solutions Found"
		title="Select Solution"
		search=".searchSolutions"
		confirm=".confirmSolution"		
		items="{
			path: '/SoftwareSolution'
		}">
		<StandardListItem
			title="{name}"
			description="{description}"			
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>