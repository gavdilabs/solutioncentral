{
	"authenticationMethod": "route",
	"routes": [
		{
			"source": "^/resources/(.*)$",
			"target": "/resources/$1",
			"destination": "UI5",
			"authenticationType": "none"
		},
		{
			"source": "^/RadarService/(.*)$",
			"target": "/odata/v4/radar/$1",
			"destination": "srv-api",
			"csrfProtection": true,
			"authenticationType": "xsuaa"
		},
		{
			"source": "^(.*)$",
			"target": "$1",
			"service": "html5-apps-repo-rt",
			"authenticationType": "xsuaa"
		}
	]
}
